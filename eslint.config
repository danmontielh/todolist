{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
    'no-unused-vars': 0,
    'no-undef': 0,
    'no-trailing-spaces': 0,
    'no-unused-expressions': 0,
    'arrow-body-style': 0,
    'no-underscore-dangle': 0,
    'no-restricted-globals': 0,
    'no-alert': 0,
    'prefer-template': 0,
    'no-plusplus': 0,
    'quotes': 0,
    'no-use-before-define': 0

  }
}